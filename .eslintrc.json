{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "no-plusplus": 0,
    "no-param-reassign": 1
  }
}